{
  "name": "systemet",
  "description": "systemet",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.systembolaget.se/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "host_permissions": [
    "https://www.hachette-vins.com/",
    "https://untappd.com/"
  ],
  "permissions": ["storage"],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
   "128": "icon-128.png"
  }
}